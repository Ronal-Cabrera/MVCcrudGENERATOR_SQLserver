
@model IEnumerable<CRUDmvc.Models.usuarios>

@{
    ViewBag.Title = "Mostrar";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.usuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.contra)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.usuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.contra)
            </td>
            <td>
                @Html.ActionLink("Editar", "Edit", new { id = item.cod }) |
                @Html.ActionLink("Detalles", "Details", new { id = item.cod }) |
                @Html.ActionLink("Eliminar", "Delete", new { id = item.cod })
            </td>
        </tr>
    }

</table>